package practice;

public class CompareTwoNumbers20 {
	public static void main(String[] args) {
		//declare two number to compare
		int num1= 1;
		int num2 = 222;
		if(num1>num2) {
			System.out.println(num1 +" is greater tha "+num2);
		}else if (num1 < num2) {
			System.out.println(num1 + "is less than " +num2);
		}else {
			System.out.println(num1 + "is equal to "+num2);
		}
		
		
	}

}
